{
    "apps": [{
        "name": "laravel-app",
        "script": "artisan",
        "args": ["octane:start", "--server=frankenphp", "--port=8002", "--host=0.0.0.0"],
        "instances": "1",
        "wait_ready": true,
        "autorestart": false,
        "max_restarts": 1,
        "interpreter" : "php",
        "watch": true,
        "error_file": "storage/log/err.log",
        "out_file": "storage/log/out.log",
        "log_file": "storage/log/combined.log",
        "time": true
    }]
}
